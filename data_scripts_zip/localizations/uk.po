msgstr ""

#: Convo.twn_berna_and_hamish_scout.HUB0.convo.giver.STATE_0.ANSWER_2D_ALT
msgctxt "Convo.twn_berna_and_hamish_scout.HUB0.convo.giver.STATE_0.ANSWER_2D_ALT"
msgid "agent:\n"
" !neutral\n"
" !agree\n"
" Right. Let's go."
msgstr "агент:\n"
" !neutral\n"
" !agree\n"
" Правильно. Ходімо."

#: Convo.twn_berna_and_hamish_scout.HUB0.convo.giver.STATE_0.ANSWER_3A
msgctxt "Convo.twn_berna_and_hamish_scout.HUB0.convo.giver.STATE_0.ANSWER_3A"
